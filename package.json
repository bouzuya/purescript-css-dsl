{
  "name": "purescript-css-dsl",
  "version": "0.0.0",
  "description": "An internal DSL for CSS using PureScript",
  "main": "index.js",
  "scripts": {
    "build": "psc-package sources | xargs purs compile 'src/**/*.purs' 'test/**/*.purs'",
    "postinstall": "psc-package install",
    "purs": "purs",
    "psc-package": "psc-package",
    "repl": "psc-package repl -- 'test/**/*.purs'",
    "test": "node -e \"require('./output/Test.Main/index.js').main()\""
  },
  "keywords": [],
  "author": "bouzuya <m@bouzuya.net> (http://bouzuya.net/)",
  "license": "MIT",
  "devDependencies": {
    "psc-package-bin-simple": "^1.0.4",
    "purescript": "^0.12.0"
  }
}
